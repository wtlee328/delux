{
  "name": "delux-plus-backend",
  "version": "1.0.0",
  "description": "Delux+ B2B2B Travel Platform Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "migrate": "npm run build && node dist/migrations/runner.js up",
    "migrate:down": "npm run build && node dist/migrations/runner.js down",
    "migrate:dev": "ts-node src/migrations/runner.ts up",
    "migrate:dev:down": "ts-node src/migrations/runner.ts down",
    "seed:admin": "ts-node src/scripts/seed-admin.ts",
    "seed:test": "ts-node src/scripts/seed-test-data.ts",
    "seed:all": "npm run seed:admin && npm run seed:test",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@google-cloud/storage": "^7.7.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "pg": "^8.11.3"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
